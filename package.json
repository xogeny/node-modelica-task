{
    "name": "@xogeny/modelica-task",
    "version": "0.9.2",
    "description": "Node module to run local Modelica simulations",
    "main": "lib/index.js",
    "scripts": {
        "start": "tsc && node lib/src/server.js",
        "compile": "tsc",
        "pushecr": "dockergen pushecr",
        "gen": "dockergen gen",
        "image": "dockergen build",
        "test": "mocha"
    },
    "keywords": [
        "modelica"
    ],
    "author": "Michael M. Tiller",
    "license": "MIT",
    "dockergen": {
        "image": "mtiller/omnode",
        "name": "xogeny/modelica-worker",
        "ecr": "897359269905.dkr.ecr.us-east-1.amazonaws.com",
        "env": {
            "REDIS_SERVER_HOST": "localhost",
            "REDIS_SERVER_PORT": 6379
        }
    },
    "dependencies": {
        "csv-parse": "^1.1.7",
        "kue": "^0.11.5",
        "temp": "^0.8.3"
    },
    "devDependencies": {
        "chai": "^3.5.0",
        "dockergen": "^0.3.4",
        "mocha": "^3.2.0",
        "ts-node": "^1.7.0",
        "typescript": "^2.1.4"
    }
}
